{
  "name": "order-now-pay-later",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && swc ./src -d dist",
    "db:console": "docker compose run postgres psql -h postgres -U postgres -d order_now_pay_later",
    "db:migrate": "knex migrate:latest",
    "docker:db:migrate": "docker compose run backend npm db:migrate",
    "docker:db:migrate:undo": "docker compose run backend npx knex migrate:down",
    "rebuild:be": "docker-compose down && docker-compose up --build",
    "start": "node dist/server.js",
    "start:docker": "npm run build && npm run dev",
    "dev": "tsnd --respawn --transpile-only --exit-child src/server.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.100",
    "@tsconfig/node18": "^18.2.2",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "date-fns": "^3.0.1",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mongoose": "^8.0.3",
    "pg": "^8.11.3"
  }
}
